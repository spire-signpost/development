<!--
Template - Home Page
- arguments = title
- helper = makeUpper
- partials = footer & sitenav
-->
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{{title}}</title>
  </head>
  <body>
    <h3>{{makeUpper title}}</h3>
    <!-- add partial - sitenav -->
    {{> sitenav}}
    <p>Welcome to the home page...</p>
    <!-- add partial - footer -->
    {{> footer}}
  </body>
</html>
